{
  "name": "kiwii-server",
  "version": "1.0.0",
  "description": "open source e-commerce application",
  "main": "index.js",
  "scripts": {
    "copy": "rm -fr dist/swagger && cp -r src/swagger dist/ && cp -r src/public dist/",
    "build": "babel src --source-maps --out-dir dist",
    "dev": "npm run build && npm run copy && babel-node -- src/index.js",
    "start": "babel-node -- src/index.js",
    "mon": "nodemon --exec babel-node src/index.js",
    "test": "jest src"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vollov/kiwii.git"
  },
  "keywords": [
    "e-commerce"
  ],
  "author": "dike.zhang@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vollov/kiwii/issues"
  },
  "homepage": "https://github.com/vollov/kiwii#readme",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.14",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "ioredis": "^4.19.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mongoose": "^5.10.10",
    "mongoose-timestamp": "^0.6.0",
    "serve-favicon": "^2.5.0",
    "stripe": "^8.118.0",
    "swagger-jsdoc": "^4.3.1",
    "swagger-ui-express": "^4.1.4",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babel-jest": "^26.6.1",
    "jest": "^26.6.1",
    "regenerator-runtime": "^0.13.7"
  }
}
